{
  "name": "fs-utils",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "styles": "sass src/styles/main.scss dist/css/main.css",
    "styles:watch": "sass src/styles/main.scss dist/css/main.css -w",
    "start": "node dist/index.js",
    "start:watch": "nodemon dist/index.js --ignore data/",
    "dev": "concurrently \"npm run build:watch\" \"npm run styles:watch\" \"npm run start:watch\" --names \"BUILD,SASS,SERVER\" --prefix \"{time} [{name}]\"",
    "prettier": "prettier --check .",
    "lint": "eslint . --ext .ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "typescript": "^5.3.3"
  }
}
