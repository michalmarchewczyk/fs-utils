{
  "name": "fs-utils",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "styles": "sass src/styles/main.scss dist/css/main.css",
    "styles:watch": "sass src/styles/main.scss dist/css/main.css -w",
    "start": "node dist/index.js",
    "start:watch": "nodemon dist/index.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run styles:watch\" \"npm run start:watch\" --names \"BUILD,SASS,SERVER\" --prefix \"{time} [{name}]\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "sass": "^1.70.0",
    "typescript": "^5.3.3"
  }
}
